{
  "name": "Avocado Ripener",
  "description": "patience is a virtue",
  "version": "1.0",
  "manifest_version": 3,
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "512": "/images/icon.png"
    }
  },
  "icons": {
    "512": "/images/icon.png"
  }
}
